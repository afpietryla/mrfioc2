file evgMrm.template {
pattern { SYS,      EVG}
        {"$(SYS)", "$(EVG)"}
}

file evgSoftEvt.template {
pattern { SYS,      EVG     }
        {"$(SYS)", "$(EVG)" }
}

file evgAcTrig.template {
pattern { SYS,      EVG     }
        {"$(SYS)", "$(EVG)" }
}

file evgEvtClk.template {
