# ID = 0 --> Main transciever (upstream)
# ID = 1+ --> Fanouts
file "sfp.template"{
pattern { SYS,      EVG,     ID }
        {"$(SYS)", "$(EVG)", 0  }
        {"$(SYS)", "$(EVG)", 1  }
        {"$(SYS)", "$(EVG)", 2  }
        {"$(SYS)", "$(EVG)", 3  }
        {"$(SYS)", "$(EVG)", 4  }
        {"$(SYS)", "$(EVG)", 5  }
        {"$(SYS)", "$(EVG)", 6  }
        {"$(SYS)", "$(EVG)", 7  }
        {"$(SYS)", "$(EVG)", 8  }
}
