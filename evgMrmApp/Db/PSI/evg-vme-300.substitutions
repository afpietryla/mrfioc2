file evgDbus.db {
pattern { SYS,      EVG,      OBJ,    ID }
        {"$(SYS)", "$(EVG)", "Dbus",  0  }
        {"$(SYS)", "$(EVG)", "Dbus",  1  }
        {"$(SYS)", "$(EVG)", "Dbus",  2  }
        {"$(SYS)", "$(EVG)", "Dbus",  3  }
        {"$(SYS)", "$(EVG)", "Dbus",  4  }
        {"$(SYS)", "$(EVG)", "Dbus",  5  }
        {"$(SYS)", "$(EVG)", "Dbus",  6  }
        {"$(SYS)", "$(EVG)", "Dbus",  7  }

}

file evgInput.db {
pattern { SYS,      EVG,      OBJ,       ID,  PPS,  DBUSOBJ}
        {"$(SYS)", "$(EVG)", "FrontInp", 0,   1,    "Dbus"}
        {"$(SYS)", "$(EVG)", "FrontInp", 1,   2,    "Dbus"}
        {"$(SYS)", "$(EVG)", "FrontInp", 2,   3,    "Dbus"}
        {"$(SYS)", "$(EVG)", "RearInp",  0,   0,    "Dbus"}
