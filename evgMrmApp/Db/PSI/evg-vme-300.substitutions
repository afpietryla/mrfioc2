# ID = 0 --> Main transciever (upstream)
# ID = 1+ --> Fanouts
file "sfp.template"{
pattern { SYS,      EVG,      OBJ,   ID }
        {"$(SYS)", "$(EVG)", "SFP",  0  }
        {"$(SYS)", "$(EVG)", "SFP",  1  }
        {"$(SYS)", "$(EVG)", "SFP",  2  }
        {"$(SYS)", "$(EVG)", "SFP",  3  }
        {"$(SYS)", "$(EVG)", "SFP",  4  }
        {"$(SYS)", "$(EVG)", "SFP",  5  }
        {"$(SYS)", "$(EVG)", "SFP",  6  }
        {"$(SYS)", "$(EVG)", "SFP",  7  }
        {"$(SYS)", "$(EVG)", "SFP",  8  }
}
